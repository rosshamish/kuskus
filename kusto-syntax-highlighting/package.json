{
  "name": "kuskus-kusto-syntax-highlighting",
  "displayName": "[Kuskus] Kusto Syntax Highlighting",
  "description": "Kusto Syntax Highlighting for Visual Studio Code.",
  "version": "1.1.18",
  "publisher": "rosshamish",
  "license": "Apache-2.0",
  "homepage": "https://github.com/rosshamish/kuskus",
  "repository": {
    "type": "git",
    "url": "https://github.com/rosshamish/kuskus"
  },
  "bugs": {
    "url": "https://github.com/rosshamish/kuskus/issues"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages",
    "Languages",
    "Language Packs"
  ],
  "keywords": [
    "kusto",
    "azure",
    "language",
    "highlight",
    "syntax"
  ],
  "contributes": {
    "languages": [
      {
        "id": "kusto",
        "aliases": [
          "Kusto",
          "kusto"
        ],
        "extensions": [
          ".csl",
          ".kusto",
          ".kql"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kusto",
        "scopeName": "source.kusto",
        "path": "./syntaxes/kusto.tmLanguage"
      }
    ]
  },
  "devDependencies": {
    "vscode-tmgrammar-test": "^0.0.11"
  },
  "scripts":{
    "test": "vscode-tmgrammar-snap --grammar syntaxes/kusto.tmLanguage --scope source.kusto --testcases 'test/snapshots/src/*'",
    "test:update-snapshots": "vscode-tmgrammar-snap --updateSnapshot --grammar syntaxes/kusto.tmLanguage --scope source.kusto --testcases 'test/snapshots/src/*'"
  }
}
